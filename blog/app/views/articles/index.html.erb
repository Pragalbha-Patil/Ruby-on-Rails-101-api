<h1 class="text-center">List of all blogs</h1>
<div>
  <ul class="list-group">
    <% @articles.each do |article| %>
      <li class="list-group-item">
        <%= link_to article.title, article %>
      </li>
    <% end %>
  </ul>
</div>
<div>
  <%= link_to "New Article", new_article_path, class:"btn btn-primary" %>
</div>